[user]
	name = Tamlyn Rhodes
	email = tamlyn@tamlyn.org
[color]
    branch = auto
    diff = auto
    status = auto
[color "branch"]
    current = yellow reverse
    local = yellow
    remote = green
[color "diff"]
    meta = yellow bold
    frag = magenta bold
    old = red bold
    new = green bold
[color "status"]
    added = yellow
    changed = green
    untracked = cyan
[push]
	default = simple
[core]
	autocrlf = input
[alias]
	s = status
	lg = log --all --decorate --oneline --graph